<body>
    <% include js_files %> 
    
<% include navbar %>



        
        
    <section class="section" id="texas-holdem-section">

            
            <div class="col-lg-12 text-center" style="height: 100px;">
                <h2>Websocket Texas-Hold'em</h2>
                <h3 id="player-name"></h3>
                <form id="add-player-form">
                    <label>Enter a name to join the game</label></br>
                    <input id="player-name-to-add" type="text" class="form-control" style="max-width: 275px; margin:auto; width:95%;"></input></br>
                    <input class="btn" id="add-player" type="submit" value="Join The Game">
                </form>
                <button class="btn" id="deal-hands">Deal Hands</button>
                <button class="btn" id="next-table-cards"></button></br>
                <button class="btn" id="new-hand">New Hand</button>
                <h1 id="hand"></h1>
                <h1 id="flop"></h1>
                <h1 id="turn"></h1>
                <h1 id="river"></h1>
                <h3>Your Hand</h3>
                <img class="pop-img-effect-2 card" id="card1" src="" onerror="this.style.display='none';">
                <img class="pop-img-effect-2 card" id="card2" src="" onerror="this.style.display='none';">
                <h3>Table</h3>
                
                <img class="pop-img-effect-2 card" id="table_card1" src="" onerror="this.style.display='none';">
                <img class="pop-img-effect-2 card" id="table_card2" src="" onerror="this.style.display='none';">
                <img class="pop-img-effect-2 card" id="table_card3" src="" onerror="this.style.display='none';">
                <img class="pop-img-effect-2 card" id="table_card4" src="" onerror="this.style.display='none';">
                <img class="pop-img-effect-2 card" id="table_card5" src="" onerror="this.style.display='none';">
                <h3 id="winner"></h3>
		<h4 id="winner-name"></h4>
		<h4 id="winner-hand-name"></h4>
                <img class="pop-img-effect-2 card" id="winner_card1" src="" onerror="this.style.display='none';">
                <img class="pop-img-effect-2 card" id="winner_card2" src="" onerror="this.style.display='none';">
                <p id="current-players"></p>
                <p id="spectator-count"></p>
            </div>
      
    </section>        
        
<% include footer %>

