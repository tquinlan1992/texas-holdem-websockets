    <style>
h1 {
  font-size:10px;
  
}
.animate {
    animation-duration: 3s;
    animation-name: slidein;
}

@keyframes slidein {
  from {
    font-size:60px;
    margin-left: 100%;
    width: 300%; 
  }

  to {
    font-size:10px;
    margin-left: 0%;
    width: 100%;
  }
}
    </style>

